{
    "name": "zettelkasten-tools",
    "displayName": "Zettelkasten Tools",
    "description": "Extensions to make working with zettelkasten nicer",
    "publisher": "antonburger",
    "version": "0.0.1",
    "repository": "https://github.com/antonburger/zettel-tools",
    "engines": {
        "vscode": "^1.44.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:zettelkasten-tools.pasteAttachment"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "zettelkasten-tools.pasteAttachment",
                "title": "Paste Attachment"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "zettelkasten-tools.pasteAttachment",
                    "when": "editorLangId == markdown"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "package": "npx vsce package",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.44.0",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.1",
        "typescript": "^3.8.3",
        "vsce": "^1.75.0",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "child_process": "^1.0.2",
        "path": "^0.12.7"
    }
}
